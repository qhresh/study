<?php
function Process($data)
{
	$start=time();
	for($i = 0; $i < 5000; $i++)
	{
		file_put_contents("/var/www/demo/debug2.log", $data["name"] . "第" . ($i+1) . "次执行：" . date("H:i:s") . "\r\n", FILE_APPEND | LOCK_EX);
	}
	$s_t = microtime(true) - $start;
	file_put_contents("/var/www/demo/debug2.log", "共耗时：" . $s_t . "秒\r\n", FILE_APPEND | LOCK_EX);
}
