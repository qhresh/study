<?php
// 创建一个cURL句柄
$ch = curl_init();
$data = array('FUNC' => 'KEY1', 'name' => 'data');
curl_setopt($ch, CURLOPT_URL, "http://localhost/entrance.php ");
curl_setopt($ch, CURLOPT_POST, 1);
curl_setopt($ch, CURLOPT_POSTFIELDS, $data);
// 执行
curl_exec($ch);

echo curl_getinfo($ch, CURLINFO_TOTAL_TIME) . "\r\n";

// Close handle
curl_close($ch);
